.spallframe{
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  z-index: 3;
  box-sizing: border-box;
}
.frame{
  width: 100%;
  height: 100%;
  background-color: #030303;
  opacity: 0.5;
  
}
.mid{
  width: 80%;
  height: 500rpx;
  background-color: #fff;
  position: absolute;
  top: 300rpx;
  left: 10%;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.mid .back{
  overflow: hidden;
}
.mid .back i-icon{
  float: right;
  margin-right: 10rpx;
}
.mid .spelltitle{
  width: 100%;
  text-align: center;
  font-weight: 600;
}
.mid .condition{
  font-size: 24rpx;
  color: #999;
  text-align: center;
  margin-top: 30rpx;
}
.mid .user_headimg{
  display: flex;
  justify-content: space-between;
  padding: 60rpx 100rpx;
}
.mid .user_headimg view{
  width: 90rpx;
  height: 90rpx;
}
.mid .user_headimg view image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.mid .confirm{
  width: 80%;
  margin: 0 auto;
  background-color: #FFC600;
  line-height: 80rpx;
  text-align: center;
  border-radius: 20rpx;
  font-size: 32rpx;
}


